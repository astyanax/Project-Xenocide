<?xml version="1.0" ?>
<GUILayout xmlns="http://ceguisharp.sourceforge.net/0.5/GUILayout"  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://ceguisharp.sourceforge.net/0.5/GUILayout ..\..\..\..\Dependancies\CeGui\CeGui\GUILayout.xsd">
	<Window Type="TaharezLook.TLFrameWindow" Name="dlgStartBattlescape">
		<Property Name="Width" Value="0.5"/>
		<Property Name="Height" Value="0.3"/>
		<Property Name="TitlebarEnabled" Value="true"/>
		<Property Name="CloseButtonEnabled" Value="false"/>
		<Property Name="FrameEnabled" Value="true"/>
		<Property Name="RollUpState" Value="false"/>
		<Property Name="Text" Value="Start Battlescape" />

		<Window Type="TaharezLook.TLStaticText" Name="txtDetails">
			<Property Name="XPosition" Value="0.02"/>
			<Property Name="YPosition" Value="0.15"/>
			<Property Name="Width" Value="0.96"/>
			<Property Name="Height" Value="0.55"/>
			<Property Name="Text" Value="" />
			<Property Name="HorzFormatting" Value="WordWrapLeftAligned"/>
			<Property Name="Font" Value="XenoSmall"/>
			<Property Name="FrameEnabled" Value="false" />
		</Window>
		<Window Type="TaharezLook.TLButton" Name="btnOk">
			<Property Name="XPosition" Value="0.10"/>
			<Property Name="YPosition" Value="0.85"/>
			<Property Name="Width" Value="0.30"/>
			<Property Name="Height" Value="0.10"/>
			<Property Name="Text" Value="Ok" />
			<Event Name="Clicked" Function="OnOkClicked"/>
		</Window>
		<Window Type="TaharezLook.TLButton" Name="btnCancel">
			<Property Name="XPosition" Value="0.50"/>
			<Property Name="YPosition" Value="0.85"/>
			<Property Name="Width" Value="0.30"/>
			<Property Name="Height" Value="0.10"/>
			<Property Name="Text" Value="Cancel" />
			<Event Name="Clicked" Function="OnCancelClicked"/>
		</Window>
    <Window Type="TaharezLook.TLButton" Name="btnAutoComplete">
      <Property Name="XPosition" Value="0.1"/>
      <Property Name="YPosition" Value="0.55"/>
      <Property Name="Width" Value="0.70"/>
      <Property Name="Height" Value="0.10"/>
      <Property Name="Text" Value="Auto Complete Battlescape" />
      <Event Name="Clicked" Function="OnAutoCompleteClicked"/>
    </Window>
  </Window>
</GUILayout>
